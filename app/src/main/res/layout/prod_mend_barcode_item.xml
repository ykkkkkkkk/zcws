<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:orientation="horizontal">


    <TextView
        android:id="@+id/tv_row"
        android:layout_width="32dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="行号"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_stkDate"
        android:layout_width="70dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="订单日期"
        android:maxLength="10"
        android:textColor="@color/black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_fbillNo"
        android:layout_width="76dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="订单号"
        android:textColor="@color/black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_orderDate"
        android:layout_width="70dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="订单日期"
        android:maxLength="10"
        android:textColor="@color/black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_deliveryWay"
        android:layout_width="56dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="发货类别"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_mtlName"
        android:layout_width="180dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="物料名称"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_fqty"
        android:layout_width="58dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="订单数量"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_lockQty"
        android:layout_width="58dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="锁库数量"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_custName"
        android:layout_width="80dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="客户"
        android:textColor="@color/black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_deliveryQty"
        android:layout_width="65dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="发货通知数"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_pickFqty"
        android:layout_width="58dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="调拨数"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_brandName"
        android:layout_width="68dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="品牌"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_seriesName"
        android:layout_width="68dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="序列"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_productName"
        android:layout_width="68dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="商品名"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_carSeriesName"
        android:layout_width="68dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="车系"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_carTypeName"
        android:layout_width="68dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="车型"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_colorName"
        android:layout_width="68dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="颜色"
        android:textColor="#000000"
        android:textSize="12sp" />


</LinearLayout>
